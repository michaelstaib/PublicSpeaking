+import {Accordion, AccordionSummary, Stack, Typography} from '@mui/material';
+
+import {ExpandLockedIcon, ExpandMoreIcon} from '@/icons';
+
+export const Group = ({
+  id,
+  icon: Icon,
+  title,
+  active,
+  disabled,
+  children,
+  onChange,
+}) => (
+  <Accordion
+    expanded={id === active}
+    disabled={disabled}
+    onChange={(event, expanded) => {
+      onChange(expanded ? id : false);
