+                <Typography id={labelId} variant="caption">
+                  {asset.name}
+                </Typography>
+                <Typography variant="caption" color="text.secondary">
+                  {asset.symbol}
+                </Typography>
+              </Stack>
+            </Stack>
+          </Link>
+        </NextLink>
+      </TableCell>
+      <TableCell
+        align="right"
+        sx={{width: 100, paddingLeft: 0, fontWeight: 600}}
